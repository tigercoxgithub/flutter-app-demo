# Preview Environment Variables
# These variables are used when building for preview/staging

# API Configuration
API_BASE_URL=https://staging-api.flutter-demo.com
API_TIMEOUT=30000

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_ID=staging-987654321

# Debug Settings
DEBUG_MODE=true
LOG_LEVEL=debug

# Feature Flags
ENABLE_DEBUG_TOOLS=true
ENABLE_PERFORMANCE_MONITORING=false

# Build Configuration
BUILD_MODE=debug
OPTIMIZE_FOR_SIZE=false
