# Production Environment Variables
# These variables are used when building for production

# API Configuration
API_BASE_URL=https://api.flutter-demo.com
API_TIMEOUT=30000

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_ID=prod-123456789

# Debug Settings
DEBUG_MODE=false
LOG_LEVEL=error

# Feature Flags
ENABLE_DEBUG_TOOLS=false
ENABLE_PERFORMANCE_MONITORING=true

# Build Configuration
BUILD_MODE=release
OPTIMIZE_FOR_SIZE=true
